{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-extraneous-class": "off",
        "@typescript-eslint/return-await": "off",
        "no-extra-boolean-cast": "off"
    }
}
